{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true,
    "arrowFunctions": true,
    "blockBindings": true,
    "generators": true
  },
  "rules": {
    "strict": 1,
    "no-underscore-dangle": 0,
    "curly": 0,
    "no-console": 0,
    "no-multi-spaces": 1,
    "key-spacing": 1,
    "no-return-assign": 0,
    "consistent-return": 1,
    "no-shadow": 0,
    "no-comma-dangle": 0,
    "no-use-before-define": 1,
    "no-empty": 0,
    "new-parens": 0,
    "no-cond-assign": 0,
    "quotes": [2, "single", "avoid-escape"],
    "camelcase": 1,
    "new-cap": [1, { "capIsNew": false }],
    "no-undef": 2,
    "semi": 0,
    "esversion": 6
  },
  "env": {
    "node": true,
    "browser": true
  },
  "globals": {
    "IS_SERVER": true
  }
}
